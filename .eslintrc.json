{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends":[
        "standard"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
    },
    "eol-last": 0,
    "no-multiple-empty-lines": [
        "error",
        {
            "max": 1,
            "maxEOF": 0
        }
    ],
}
